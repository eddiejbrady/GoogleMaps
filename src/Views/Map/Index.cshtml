<body>

    <div id="edit-form" class="pull-left">
        <div id="effect" class="ui-widget-content ui-corner-all">
            <div>
                <form class="form-horizontal" role="search">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Search" name="srch-term" id="search-value" value="123" />
                        <div class="input-group-btn">
                            <button id="search" class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
                        </div>
                    </div>
                    <h3 class="edit-form-header">Policy Info</h3>
                    <div class="edit-form-detail">
                        <div class="form-group row">
                            <label for="policy-id" class="col-sm-3 control-label">Policy Id</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="policy-id" placeholder="" disabled readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="policy-start-date" class="col-sm-3 control-label">Start Date</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="policy-effective-date" placeholder="" disabled readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="policy-end-date" class="col-sm-3 control-label">End Date</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="policy-expiration-date" placeholder="" disabled readonly />
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <div class="edit-form-content">
                <form class="form-horizontal">
                    <ul class="nav nav-tabs nav-justified">
                        <li class="active"><a id="policy-tab" data-toggle="tab" href="#policy-content">Policy</a></li>
                        <li><a id="group-tab" data-toggle="tab" href="#group-content">Groups</a></li>
                    </ul>

                    <div class="tab-content">
                        <div id="policy-content" class="tab-pane fade in active">
                            <h3 class="edit-form-header">Policy Objects</h3>
                            <div class="edit-form-detail building-list">
                                <table id="building-list" class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>Building Id</th>
                                            <th>Address</th>
                                            <th>&nbsp;</th>
                                            <th>&nbsp;</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>

                            <h3 class="edit-form-properties">Properties</h3>
                            <div class="edit-form-detail">
                                <div class="form-group row">
                                    <label for="building-id" class="col-sm-4 control-label">Building Id</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" id="building-id" placeholder="" disabled readonly />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label for="building-construction-type" class="col-sm-4 control-label">Construction Type</label>
                                    <div class="col-sm-8">
